{
    "name": "freshcells/guzzle-message-anonymizer-formatter",
    "description": "Anonymize Request data from guzzle before logging",
    "license": "MIT",
    "type": "library",
    "authors": [
        { "name": "Ivo Bathke", "email": "ivo.bathke@gmail.com" }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^6.1"
    },
    "suggest": {
        "csa/guzzle-bundle": "Guzzle bundle for symfony"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "jeroen/psr-log-test-doubles": "^2.2|^3.0"
    },
    "autoload": {
        "psr-4": { "Freshcells\\GuzzleMessageAnonymizerFormatter\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Freshcells\\Tests\\GuzzleMessageAnonymizerFormatter\\": "tests" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-main": "1.3-dev"
        }
    }
}
